{
  "name": "contractlab",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm -r build",
    "build:packages": "pnpm -r --filter='./packages/*' build",
    "build:types": "pnpm --filter=@contractlab/types build",
    "build:db": "pnpm --filter=@contractlab/db build",
    "build:api": "pnpm --filter=api build",
    "build:web": "pnpm --filter=web build",
    "dev:api": "pnpm --filter=api dev",
    "dev:web": "pnpm --filter=web dev",
    "dev:types": "pnpm --filter=@contractlab/types dev",
    "dev:db": "pnpm --filter=@contractlab/db dev",
    "db:generate": "pnpm --filter=@contractlab/db db:generate",
    "db:push": "pnpm --filter=@contractlab/db db:push",
    "db:studio": "pnpm --filter=@contractlab/db db:studio",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.1"
}
